javascript:(function() {  var imageURL = 'https://i.imgur.com/eYF9nfR.png';  var animationDuration = '0.5s';  var overlay = document.createElement('div');  overlay.style.position = 'fixed';  overlay.style.top = '0';  overlay.style.left = '0';  overlay.style.width = '100%';  overlay.style.height = '100%';  overlay.style.backgroundImage = 'url(' + imageURL + ')';  overlay.style.backgroundSize = 'cover';  overlay.style.opacity = '0';  overlay.style.transition = 'opacity ' + animationDuration;  function showOverlay() {    overlay.style.opacity = '1';  }  function hideOverlay() {    overlay.style.opacity = '0';    setTimeout(function() {      document.body.removeChild(overlay);    }, parseFloat(animationDuration) * 1000);  }  document.addEventListener('keydown', function(event) {    if (event.key === 'Escape') {      hideOverlay();    }  });  document.body.appendChild(overlay);  document.title = 'Google Classroom';  var link = document.querySelector("link[rel*='icon']") || document.createElement('link');  link.type = 'image/png';  link.rel = 'icon';  link.href = 'https://upload.wikimedia.org/wikipedia/commons/5/59/Google_Classroom_Logo.png';  document.head.appendChild(link);  showOverlay();})();
